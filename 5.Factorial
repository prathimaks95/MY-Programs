def fact(n):
    if n==0 or n==1:
        return 1
    else:
        return fact(n-1)*n

def Factornot(n):
    fac=1
    i=1
    while(fac<n):
        i=i+1
        fac=fact(i)
    if(fac==n):
        return ("is a factorial number")
    else:
        return ("not a factorial no")

print(fact(3))
print(Factornot(6))
print(Factornot(5))
